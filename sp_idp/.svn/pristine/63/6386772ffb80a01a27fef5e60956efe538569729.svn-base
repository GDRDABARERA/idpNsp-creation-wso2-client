<IdentityProviders>
    <IdentityProvider>
        <IdentityProviderName>adfs-federated-idp</IdentityProviderName>
        <DisplayName></DisplayName>
        <IdentityProviderDescription></IdentityProviderDescription>
        <Alias>https://dev5.account.verifone.com:9443/oauth2/token/</Alias>
        <IsPrimary></IsPrimary>
        <IsFederationHub></IsFederationHub>
        <HomeRealmId></HomeRealmId>
        <ProvisioningRole></ProvisioningRole>
        <FederatedAuthenticatorConfigs>
            <saml2>
                <Name>SAMLSSOAuthenticator</Name>
                <DisplayName>samlsso</DisplayName>
                <IsEnabled>true</IsEnabled>
                <Properties>
                    <property>
                        <Name>IdpEntityId</Name>
                        <Value>https://samlidp.verifone.com/idp/shibboleth</Value>
                    </property>
                    <property>
                        <Name>IsLogoutEnabled</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>SPEntityId</Name>
                        <Value>verifone-wso2-identity</Value>
                    </property>
                    <property>
                        <Name>SSOUrl</Name>
                        <Value>https://localhost:9445/samlsso</Value>
                    </property>
                    <property>
                        <Name>isAssertionSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>commonAuthQueryParams</Name>
                        <Value></Value>
                    </property>
                    <property>
                        <Name>IsUserIdInClaims</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsLogoutReqSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAssertionEncrypted</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAuthReqSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAuthnRespSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>LogoutReqUrl</Name>
                        <Value></Value>
                    </property>
                </Properties>
            </saml2>

        </FederatedAuthenticatorConfigs>
        <DefaultAuthenticatorConfig>
        </DefaultAuthenticatorConfig>
        <ProvisioningConnectorConfigs>
        </ProvisioningConnectorConfigs>
        <DefaultProvisioningConnectorConfig></DefaultProvisioningConnectorConfig>
        <ClaimConfig>
            <claimMappings>
                <localClaim>
                    <claimId>1</claimId>
                    <claimUri>http://wso2.org/claims/emailaddress</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>1</claimId>
                    <claimUri>verifoneEmpUPN</claimUri>
                </remoteClaim>
                <requested></requested>
            </claimMappings>
            <claimMappings>
                <localClaim>
                    <claimId>2</claimId>
                    <claimUri>http://wso2.org/claims/givenname</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>2</claimId>
                    <claimUri>verifoneFN</claimUri>
                </remoteClaim>
                <requested></requested>
            </claimMappings>
            <claimMappings>
                <localClaim>
                    <claimId>3</claimId>
                    <claimUri>http://wso2.org/claims/lastname</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>3</claimId>
                    <claimUri>verifoneLN</claimUri>
                </remoteClaim>
                <requested></requested>
            </claimMappings>
            <claimMappings>
                <localClaim>
                    <claimId>4</claimId>
                    <claimUri>http://wso2.org/claims/streetaddress</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>4</claimId>
                    <claimUri>verifoneLocation</claimUri>
                </remoteClaim>
                <requested></requested>
            </claimMappings>
            <idpClaims>
                <claimId>1</claimId>
                <claimUri>verifoneEmpUPN</claimUri>
            </idpClaims>
            <idpClaims>
                <claimId>2</claimId>
                <claimUri>verifoneFN</claimUri>
            </idpClaims>
            <idpClaims>
                <claimId>3</claimId>
                <claimUri>verifoneLN</claimUri>
            </idpClaims>
            <idpClaims>
                <claimId>4</claimId>
                <claimUri>verifoneLocation</claimUri>
            </idpClaims>
            <localClaimDialect>false</localClaimDialect>
            <userClaimURI>verifoneEmpUPN</userClaimURI>

        </ClaimConfig>
        <Certificate>adfs.prod.crt</Certificate>
        <PermissionAndRoleConfig></PermissionAndRoleConfig>
        <JustInTimeProvisioningConfig>
            <provisioningEnabled>true</provisioningEnabled>
            <provisioningUserStore>adfs</provisioningUserStore>
            <userStoreClaimUri></userStoreClaimUri>
        </JustInTimeProvisioningConfig>
    </IdentityProvider>


    <IdentityProvider>
        <IdentityProviderName>ums-idp</IdentityProviderName>
        <DisplayName></DisplayName>
        <IdentityProviderDescription></IdentityProviderDescription>
        <Alias>https://dev5.account.verifone.com:9443/oauth2/token/</Alias>
        <IsPrimary></IsPrimary>
        <IsFederationHub></IsFederationHub>
        <HomeRealmId></HomeRealmId>
        <ProvisioningRole></ProvisioningRole>
        <FederatedAuthenticatorConfigs>
            <saml2>
                <Name>SAMLSSOAuthenticator</Name>
                <DisplayName>samlsso</DisplayName>
                <IsEnabled>false</IsEnabled>
                <Properties>
                    <property>
                        <Name>IdpEntityId</Name>
                        <Value></Value>
                    </property>
                    <property>
                        <Name>IsLogoutEnabled</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>SPEntityId</Name>
                        <Value></Value>
                    </property>
                    <property>
                        <Name>SSOUrl</Name>
                        <Value></Value>
                    </property>
                    <property>
                        <Name>isAssertionSigned</Name>
                        <Value>true</Value>
                    </property>
                    <property>
                        <Name>commonAuthQueryParams</Name>
                        <Value></Value>
                    </property>
                    <property>
                        <Name>IsUserIdInClaims</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsLogoutReqSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAssertionEncrypted</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAuthReqSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>IsAuthnRespSigned</Name>
                        <Value>false</Value>
                    </property>
                    <property>
                        <Name>LogoutReqUrl</Name>
                        <Value></Value>
                    </property>
                </Properties>
            </saml2>

        </FederatedAuthenticatorConfigs>
        <DefaultAuthenticatorConfig>
        </DefaultAuthenticatorConfig>
        <ProvisioningConnectorConfigs>
            <ProvisioningConnectorConfig>
                <blocking>true</blocking>
                <enabled>true</enabled>
                <name>Developer-portal</name>
            </ProvisioningConnectorConfig>
        </ProvisioningConnectorConfigs>
        <DefaultProvisioningConnectorConfig></DefaultProvisioningConnectorConfig>
        <ClaimConfig>
        </ClaimConfig>
        <Certificate></Certificate>
        <PermissionAndRoleConfig></PermissionAndRoleConfig>
        <JustInTimeProvisioningConfig>
        </JustInTimeProvisioningConfig>
    </IdentityProvider>

</IdentityProviders>
