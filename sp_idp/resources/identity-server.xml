<IdentityProviders>
    <IdentityProvider>
        <IdentityProviderName>MyIDPTarvis</IdentityProviderName>
        <DisplayName>MyIDPTarvis</DisplayName>
        <IdentityProviderDescription>idp for the travis</IdentityProviderDescription>
        <enable>true</enable>
        <Alias>https://dev5.account.verifone.com:9443/oauth2/token</Alias>
        <IsPrimary></IsPrimary>
        <IsFederationHub></IsFederationHub>
        <HomeRealmId></HomeRealmId>
        <ProvisioningRole></ProvisioningRole>
        <DefaultAuthenticatorConfig>
        </DefaultAuthenticatorConfig>

        <DefaultProvisioningConnectorConfig>
            <enabled>true</enabled>
            <Name>scim</Name>
            <ProvisioningProperties>
                <Property>
                    <name>scim</name>
                    <value>true</value>
                </Property>
                <Property>
                    <name>scimProvDefault</name>
                    <value>on</value>
                </Property>
                <Property>
                    <name>scim-username</name>
                    <value>admin</value>
                </Property>
                <Property>
                    <name>scim-password</name>
                    <value>admin</value>
                </Property>
                <Property>
                    <name>scim-user-ep</name>
                    <value>https://localhost:9445/wso2/scim/Users</value>
                </Property>
                <Property>
                    <name>scim-group-ep</name>
                    <value>https://localhost:9445/wso2/scim/Groups</value>
                </Property>
                <Property>
                    <name>scim-user-store-domain</name>
                    <value></value>
                </Property>
                <Property>
                    <name>scimPwdProvEnabled</name>
                    <value>false</value>
                </Property>
                <Property>
                    <name>scim-default-pwd</name>
                    <value>123456</value>
                </Property>
            </ProvisioningProperties>

        </DefaultProvisioningConnectorConfig>

        <ProvisioningConnectorConfigs>
            <ProvisioningConnectorConfig>
                <enabled>true</enabled>
                <Name>scim</Name>
                <ProvisioningProperties>
                    <Property>
                        <name>scim</name>
                        <value>true</value>
                    </Property>
                    <Property>
                        <name>scimProvDefault</name>
                        <value>on</value>
                    </Property>
                    <Property>
                        <name>scim-username</name>
                        <value>admin</value>
                    </Property>
                    <Property>
                        <name>scim-password</name>
                        <value>admin</value>
                    </Property>
                    <Property>
                        <name>scim-user-ep</name>
                        <value>https://localhost:9445/wso2/scim/Users</value>
                    </Property>
                    <Property>
                        <name>scim-group-ep</name>
                        <value>https://localhost:9445/wso2/scim/Groups</value>
                    </Property>
                    <Property>
                        <name>scim-user-store-domain</name>
                        <value></value>
                    </Property>
                    <Property>
                        <name>scimPwdProvEnabled</name>
                        <value>false</value>
                    </Property>
                    <Property>
                        <name>scim-default-pwd</name>
                        <value>123456</value>
                    </Property>
                </ProvisioningProperties>
            </ProvisioningConnectorConfig>
        </ProvisioningConnectorConfigs>

        <PermissionsAndRoleConfig>
            <RoleMappings>
                <!--{% for item in role_mappings -%} -->
                <RoleMapping>
                    <localRole>provision</localRole>
                    <remoteRole>role3</remoteRole>
                </RoleMapping>
              <!--  {% endfor -%} -->
            </RoleMappings>
        </PermissionsAndRoleConfig>
        <ClaimConfig></ClaimConfig>
        <Certificate>adfs.prod.crt</Certificate>
        <JustInTimeProvisioningConfig>
            <provisioningEnabled>true</provisioningEnabled>
            <provisioningUserStore>adfs</provisioningUserStore>
            <userStoreClaimUri></userStoreClaimUri>
        </JustInTimeProvisioningConfig>
    </IdentityProvider>
</IdentityProviders>