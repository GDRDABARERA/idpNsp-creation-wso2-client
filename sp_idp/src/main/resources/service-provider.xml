<ServiceProviders>


    <ServiceProvider>
        <ApplicationID></ApplicationID>
        <ApplicationName>dev5.verifone.sample.9</ApplicationName>
        <Description></Description>
        <InboundAuthenticationConfig>
            <InboundAuthenticationRequestConfigs>
                <InboundAuthenticationRequestConfig>
                    <InboundAuthKey>default</InboundAuthKey>
                    <InboundAuthType>oauth2</InboundAuthType>
                    <Properties>
                        <Name>oauthConsumerSecret</Name>
                        <Value></Value>
                    </Properties>
                </InboundAuthenticationRequestConfig>
            </InboundAuthenticationRequestConfigs>
            <OAuthAPP>
                <Version>OAuth-2.0</Version>
                <CallBackURL>http://sp.verifone.com:8080/dev5.verifone.sample.two/oauth2client</CallBackURL>
                <GrantTypes>authorization_code implicit password client_credentials refresh_token urn:ietf:params:oauth:grant-type:saml2-bearer iwa:ntlm</GrantTypes>
                <UserName>admin</UserName>
            </OAuthAPP>
        </InboundAuthenticationConfig>
        <LocalAndOutBoundAuthenticationConfig>
            <AuthenticationSteps>
                <AuthenticationStep>
                    <StepOrder>1</StepOrder>
                    <LocalAuthenticatorConfigs>
                        <LocalAuthenticatorConfig>
                            <Name>BasicAuthenticator</Name>
                            <DisplayName>basic</DisplayName>
                            <IsEnabled>true</IsEnabled>
                        </LocalAuthenticatorConfig>
                    </LocalAuthenticatorConfigs>
                    <FederatedIdentityProviders>
                        <IdentityProvider>
                            <IdentityProviderName>adfs-federated-idp</IdentityProviderName>
                            <IsEnabled>true</IsEnabled>
                            <DefaultAuthenticatorConfig>
                                <FederatedAuthenticatorConfig>
                                    <Name>adfs-federated-idp</Name>
                                    <IsEnabled>true</IsEnabled>
                                </FederatedAuthenticatorConfig>
                            </DefaultAuthenticatorConfig>
                        </IdentityProvider>
                    </FederatedIdentityProviders>
                    <SubjectStep>true</SubjectStep>
                    <AttributeStep>true</AttributeStep>
                </AuthenticationStep>
                <AuthenticationStep>
                    <StepOrder>2</StepOrder>
                    <LocalAuthenticatorConfigs>
                        <LocalAuthenticatorConfig>
                            <Name>SignupInterceptAuthenticator</Name>
                            <DisplayName>signup</DisplayName>
                            <IsEnabled>true</IsEnabled>
                        </LocalAuthenticatorConfig>
                    </LocalAuthenticatorConfigs>
                    <FederatedIdentityProviders>
                    </FederatedIdentityProviders>
                    <SubjectStep>false</SubjectStep>
                    <AttributeStep>false</AttributeStep>
                </AuthenticationStep>
            </AuthenticationSteps>
        </LocalAndOutBoundAuthenticationConfig>
        <RequestPathAuthenticatorConfigs></RequestPathAuthenticatorConfigs>
        <InboundProvisioningConfig></InboundProvisioningConfig>
        <OutboundProvisioningConfig>
            <IdentityProviderNames>
                <IdentityProviderName>SilvaIDP</IdentityProviderName>
            </IdentityProviderNames>
        </OutboundProvisioningConfig>
        <ClaimConfig>
            <claimMappings>
                <localClaim>
                    <claimId>1</claimId>
                    <claimUri>http://wso2.org/claims/emailaddress</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>1</claimId>
                    <claimUri>http://common.verifone.com/claims/emailaddress</claimUri>
                </remoteClaim>
                <requested>true</requested>
            </claimMappings>
            <claimMappings>
                <localClaim>
                    <claimId>2</claimId>
                    <claimUri>http://wso2.org/claims/givenname</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>2</claimId>
                    <claimUri>http://common.verifone.com/claims/givenname</claimUri>
                </remoteClaim>
                <requested>true</requested>
            </claimMappings>
	    <claimMappings>
                <localClaim>
                    <claimId>3</claimId>
                    <claimUri>http://wso2.org/claims/userid</claimUri>
                </localClaim>
                <remoteClaim>
                    <claimId>3</claimId>
                    <claimUri>http://common.verifone.com/claims/userid</claimUri>
                </remoteClaim>
                <requested>true</requested>
            </claimMappings>
            <idpClaims>
                <claimId>1</claimId>
                <claimUri>verifoneEmpUPN</claimUri>
            </idpClaims>
            <idpClaims>
                <claimId>2</claimId>
                <claimUri>verifoneFN</claimUri>
            </idpClaims>
            <localClaimDialect>false</localClaimDialect>
        </ClaimConfig>
        <PermissionAndRoleConfig></PermissionAndRoleConfig>
    </ServiceProvider>
	

